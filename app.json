{
  "expo": {
    "name": "Dichvunuoc: Nhân viên",
    "slug": "dichvunuoc_nhanvien",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/LogoApp/Logo_256_256.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dichvunuoc.nhanvien",
      "buildNumber": "3"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.viboy196.dichvunuoc_nhanvien",
      "versionCode": 3
    },
    "web": {
      "image": "./assets/images/splash.png"
    }
  }
}
